{
  "devDependencies": {
    "@lhci/cli": "0.15.1",
    "prettier": "3.7.4"
  },
  "name": "website",
  "private": true,
  "scripts": {
    "audit": "lhci autorun",
    "build": "rm -rf dist; mkdir dist; cp bruno-roque-resume.pdf favicon.svg index.html dist",
    "fmt": "prettier --ignore-path .gitignore -w .",
    "fmt:check": "prettier --ignore-path .gitignore -l .",
    "resume": "typst compile resume.typ bruno-roque-resume.pdf",
    "resume:watch": "typst watch resume.typ bruno-roque-resume.pdf"
  },
  "version": "0.0.0"
}
